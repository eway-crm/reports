    <DataSet Name="Users">
      <Query>
        <DataSourceName>LocalDatabase</DataSourceName>
        <CommandText>select ItemId, FileAs from
(
	select '11111111-1111-1111-1111-111111111111' as ItemId, '('+dbo.Translate('All')+')' as FileAs
	union
	SELECT ItemId, FileAs
	  FROM [dbo].[Users]
	  where Active = 1 and IsSystem=0
) x
 order by FileAs</CommandText>
