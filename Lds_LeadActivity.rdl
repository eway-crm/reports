    <DataSet Name="Main">
      <Query>
        <DataSourceName>LocalDatabase</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@SalesManager">
            <Value>=Parameters!SalesManager.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@LeadType">
            <Value>=Parameters!LeadType.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@From">
            <Value>=Parameters!From.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@To">
            <Value>=Parameters!To.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ICL">
            <Value>=Parameters!ICL.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>DECLARE @Leads TABLE(
	ItemID UNIQUEIDENTIFIER,
	FileAs NVARCHAR(256),
	Customer NVARCHAR(256),
	ContactPerson NVARCHAR(1280),
	En NVARCHAR(256),
	StateEn UNIQUEIDENTIFIER,
	ItemCreated DATETIME,
	Price FLOAT,
	Probability FLOAT,
	OwnerName NVARCHAR(1280),
	Link NVARCHAR(256),
	Currency NVARCHAR(256)
);

INSERT INTO @Leads
SELECT 
	Leads.ItemID, Leads.FileAs, dbo.GetLeadCustomer(Leads.ItemID), dbo.GetLeadContactPerson(Leads.ItemID), 
	dbo.TranslateEnum(EnumValues.ItemID) en, EnumValues.ItemID, Leads.ItemCreated, Leads.Price, Leads.Probability, Users.FileAs,
	'eway://Leads/' + CAST(Leads.ItemID AS NVARCHAR(256)) AS Link,
	EnumValuesCurrency.en
FROM Leads
	INNER JOIN EnumValues 
		ON Leads.StateEn = EnumValues.ItemID
			AND EnumValues.ItemState &lt;&gt; 3
			AND (@SalesManager = '11111111-1111-1111-1111-111111111111' OR Leads.OwnerGUID = @SalesManager)
	INNER JOIN dbo.Users 
		ON Leads.ownerGUID = Users.ItemId 
			AND Users.ItemState &lt;&gt; 3
	INNER JOIN EnumValues EnumValuesCurrency ON EnumValuesCurrency.ItemID = Leads.CurrencyEn
WHERE (Leads.TypeEn = @LeadType OR @LeadType = '00000000-0000-0000-0000-000000000000')
	AND Leads.ItemState &lt;&gt; 3

-- All activities within the given time
DECLARE @Activities TABLE(
	leadGUID UNIQUEIDENTIFIER,
	itemGUID UNIQUEIDENTIFIER,
	startDate DATETIME,
	endDate DATETIME,
	itemSubject NVARCHAR(1280),
	itemType NVARCHAR(256),
	itemOwner NVARCHAR(256),
	isInFuture BIT,
	itemTypeOrder TINYINT,
	dateToView NVARCHAR(256),
	link NVARCHAR(256)
)

-- ** FUTURE Tasks &amp; Calendar

-- Tasks
INSERT INTO @Activities
	SELECT DISTINCT Leads.itemID, Tasks.ItemID, ISNULL(Tasks.StartDate, Tasks.ItemCreated), Tasks.DueDate, Tasks.FileAs, dbo.Translate('Task'), Users.FileAs, 1 ,1, NULL,
		'eWay://Tasks/' + CAST(Tasks.ItemID AS NVARCHAR(256)) AS Link
	FROM @Leads Leads
		INNER JOIN dbo.Relations LeadsToTaskRelations ON Leads.Itemid = LeadsToTaskRelations.ItemGUID1
			AND LeadsToTaskRelations.ItemState &lt;&gt; 3
		INNER JOIN dbo.Tasks ON LeadsToTaskRelations.ItemGUID2 = Tasks.ItemID
			AND Tasks.ItemState &lt;&gt; 3
		INNER JOIN dbo.Relations TasksToUsersRelations ON Tasks.ItemId = TasksToUsersRelations.ItemGUID1
			AND TasksToUsersRelations.ItemState &lt;&gt; 3
			AND TasksToUsersRelations.RelationType = 'TASKSOLVER'
		INNER JOIN dbo.[Users] ON TasksToUsersRelations.ItemGUID2 = Users.ItemID 
			AND Users.ItemState &lt;&gt; 3
	WHERE Tasks.Complete = 0

-- Calendar
INSERT INTO @Activities
	SELECT DISTINCT Leads.ItemID, Calendar.ItemID, StartDate, EndDate, Calendar.FileAs,  dbo.Translate('Calendar'), Users.FileAs, 1 ,1, NULL,
		'eWay://Calendar/' + CAST(Calendar.ItemID AS NVARCHAR(256)) AS Link
	FROM @Leads Leads
		INNER JOIN dbo.Relations LeadsToCalendarRelations ON Leads.Itemid = LeadsToCalendarRelations.ItemGUID1
			AND LeadsToCalendarRelations.ItemState &lt;&gt; 3
		INNER JOIN dbo.Calendar ON LeadsToCalendarRelations.ItemGUID2 = Calendar.ItemID
			AND Calendar.ItemState &lt;&gt; 3
		INNER JOIN dbo.[Users] ON Calendar.OwnerGUID = Users.ItemId
			AND Users.ItemState &lt;&gt; 3
	WHERE StartDate &gt;= DATEADD(DAY, DATEDIFF(DAY, 0, GETDATE()), 0) /* today's midnight */
			
			
-- ** ALL HISTORY
-- Tasks
INSERT INTO @Activities
	SELECT DISTINCT Leads.ItemID, Tasks.ItemID, ISNULL(Tasks.StartDate, Tasks.ItemCreated), Tasks.DueDate, Tasks.FileAs,  dbo.Translate('Task'), Users.FileAs, 0 ,2, NULL,
		'eWay://Tasks/' + CAST(Tasks.ItemID AS NVARCHAR(256)) AS Link
	FROM @Leads Leads
		INNER JOIN dbo.Relations LeadsToTasksRelations ON Leads.ItemID = LeadsToTasksRelations.ItemGUID1
			AND LeadsToTasksRelations.ItemState &lt;&gt; 3
		INNER JOIN dbo.Tasks ON LeadsToTasksRelations.ItemGUID2 = Tasks.ItemId
			AND Tasks.ItemState &lt;&gt; 3
		INNER JOIN dbo.Relations TasksToUsersRelations ON Tasks.ItemId = TasksToUsersRelations.ItemGUID1 
			AND TasksToUsersRelations.ItemState &lt;&gt; 3
			AND TasksToUsersRelations.RelationType = 'TASKSOLVER'
		INNER JOIN dbo.Users ON Tasks.OwnerGUID = Users.ItemID
			AND Users.ItemState &lt;&gt; 3
	WHERE Tasks.Complete = 1
		AND Tasks.StartDate BETWEEN @From AND DATEADD(dd, +1, @To)
				
			
-- Calendar
INSERT INTO @Activities
	SELECT DISTINCT Leads.ItemID, Calendar.ItemID, Calendar.StartDate, Calendar.EndDate, Calendar.FileAs,  dbo.Translate('Calendar'), Users.FileAs, 0 ,2, NULL,
		'eWay://Calendar/' + CAST(Calendar.ItemID AS NVARCHAR(256)) AS Link
	FROM @Leads Leads 
		INNER JOIN dbo.Relations LeadsToCalendarRelations ON Leads.Itemid = LeadsToCalendarRelations.ItemGUID1
			AND LeadsToCalendarRelations.ItemState &lt;&gt; 3
		INNER JOIN dbo.Calendar ON LeadsToCalendarRelations.ItemGUID2 = Calendar.ItemID
			AND Calendar.ItemState &lt;&gt; 3
		INNER JOIN dbo.Users ON Calendar.OwnerGUID = Users.ItemID
			AND Users.ItemState &lt;&gt; 3
	WHERE Calendar.StartDate &lt; DATEADD(DAY, DATEDIFF(DAY, 0, GETDATE()), 0) /* today's midnight */
		AND Calendar.StartDate BETWEEN @From AND DATEADD(dd, +1, @To)		
				
				
-- Journal
INSERT INTO @Activities
	SELECT DISTINCT Leads.ItemID, Journal.ItemID, Journal.EventStart, Journal.EventEnd, Journal.FileAs,  dbo.Translate('Journal'), Users.FileAs, 0 ,2, NULL,
		'eWay://Journal/' + CAST(Journal.ItemID AS NVARCHAR(256)) AS Link
	FROM @Leads Leads
		INNER JOIN dbo.Relations LeadsToJournalRelations ON Leads.Itemid = LeadsToJournalRelations.ItemGUID1
			AND LeadsToJournalRelations.ItemState &lt;&gt; 3
		INNER JOIN dbo.Journal ON LeadsToJournalRelations.ItemGUID2 = Journal.ItemId 
			AND Journal.ItemState &lt;&gt; 3
			AND Journal.[System] &lt;&gt; 1
		INNER JOIN dbo.Users ON Journal.OwnerGUID = Users.ItemID
			AND Users.ItemState &lt;&gt; 3
	WHERE Journal.EventStart BETWEEN @From AND DATEADD(dd, +1, @To)
				
-- Documents	
INSERT INTO @Activities
	SELECT DISTINCT Leads.ItemID, Documents.ItemID, Documents.ItemCreated, NULL, Documents.FileAs,  dbo.Translate('Document'), Users.FileAs, 0 ,2, NULL,
		'eWay://Documents/' + cast(Documents.ItemID AS NVARCHAR(256)) AS Link
	FROM @Leads Leads 
		INNER JOIN dbo.Relations LeadsToDocumentsRelations ON Leads.Itemid = LeadsToDocumentsRelations.ItemGUID1
			AND LeadsToDocumentsRelations.ItemState &lt;&gt; 3
		INNER JOIN dbo.Documents ON LeadsToDocumentsRelations.ItemGUID2 = Documents.ItemId
			AND Documents.ItemState &lt;&gt; 3
		INNER JOIN dbo.Users ON Documents.OwnerGUID = Users.ItemID
			AND Users.ItemState &lt;&gt; 3
	WHERE Documents.ItemCreated BETWEEN @From AND DATEADD(dd, +1, @To)

-- Emails	
INSERT INTO @Activities
	SELECT DISTINCT Leads.ItemID, Emails.ItemID, Emails.ItemCreated, null, Emails.FileAs,  dbo.Translate('Email'), Users.FileAs, 0 ,2, NULL,
		'eWay://Emails/' + cast(Emails.ItemID AS NVARCHAR(256)) AS Link
	FROM @Leads Leads 
		INNER JOIN dbo.Relations LeadsToEmailsRelations ON Leads.Itemid = LeadsToEmailsRelations.ItemGUID1
			AND LeadsToEmailsRelations.ItemState &lt;&gt; 3
		INNER JOIN dbo.Emails ON LeadsToEmailsRelations.ItemGUID2 = Emails.ItemId
			AND Emails.ItemState &lt;&gt; 3
		INNER JOIN dbo.Users ON Emails.OwnerGUID = Users.ItemID
			AND Users.ItemState &lt;&gt; 3
	WHERE Emails.ItemCreated BETWEEN @From and DATEADD(dd, +1, @To)	
				
-- Work Reports	
INSERT INTO @Activities
	SELECT DISTINCT Leads.ItemID, WorkReports.ItemID, WorkReports.[Date], NULL, WorkReports.FileAs,  dbo.Translate('Work Report'), Users.FileAs, 0 ,2, NULL,
		'eWay://WorkReports/' + CAST(WorkReports.ItemID AS NVARCHAR(256)) AS Link
	FROM @Leads Leads
		INNER JOIN dbo.Relations LeadsToWorkReportRelations ON Leads.Itemid = LeadsToWorkReportRelations.ItemGUID1
			AND LeadsToWorkReportRelations.ItemState &lt;&gt; 3
		INNER JOIN dbo.WorkReports ON LeadsToWorkReportRelations.ItemGUID2 = WorkReports.ItemId
			AND WorkReports.ItemState &lt;&gt; 3
		INNER JOIN dbo.Users ON WorkReports.OwnerGUID = Users.ItemID
			AND Users.ItemState &lt;&gt; 3
	WHERE WorkReports.[Date] BETWEEN @From and DATEADD(dd, +1, @To)

-- Temp table for all data
DECLARE @t TABLE
(
	-- lead details
	 leadGUID UNIQUEIDENTIFIER
	,leadName NVARCHAR(256)
	,customerName NVARCHAR(256)
	,contactPerson NVARCHAR(256)
	,leadState NVARCHAR(256)
	,leadDate DATETIME
	,leadPrice FLOAT
	,leadProbability FLOAT
	,userName NVARCHAR(255)
	,link NVARCHAR(256)
	,currency NVARCHAR(256)
	-- records
    ,itemGUID UNIQUEIDENTIFIER
	,startDate DATETIME
	,endDate DATETIME
	,itemSubject NVARCHAR(1280)
	,itemType NVARCHAR(256)
	,itemOwner NVARCHAR(256)
	,isInFuture BIT
	,itemTypeOrder TINYINT
	,dateToView NVARCHAR(256)
	,activityLink NVARCHAR(256)
	,translation NVARCHAR(50)
)

-- ALL view and ACTIVITY view contains all leads (regardless lead state) with an activity within given time 
IF ( @ICL = 'All Deals' OR @ICL = 'Deals With Activity' )
	INSERT INTO @t
		SELECT 
			l.ItemID, l.FileAs, l.Customer, l.ContactPerson, l.En, 
			l.ItemCreated, l.Price, l.Probability, l.OwnerName, l.link, l.Currency,
			a.ItemGUID, a.StartDate, a.EndDate, a.ItemSubject, a.ItemType, 
			a.ItemOwner, a.isInFuture, a.ItemTypeOrder, a.DateToView, a.link, dbo.Translate('Task')
		FROM @Leads l			
			INNER JOIN @Activities a
				ON l.ItemID = A.LeadGUID
			
-- ALL View contains (from previous IF) leads with activity, now we add leads without activity that were created within given time 
IF ( @ICL = 'All Deals' )
	INSERT INTO @t
		SELECT 
			l.ItemID, l.FileAs, l.Customer, l.ContactPerson, l.En, 
			l.ItemCreated, l.Price, l.Probability, l.OwnerName, l.link, l.Currency,
			null, null, null, null, null, null, null, null, null, null, dbo.Translate('Task')
		FROM @Leads l
		WHERE l.ItemID NOT IN ( SELECT LeadGUID FROM @Activities )
			AND l.ItemCreated BETWEEN @From and DATEADD(dd, +1, @To) -- Within given time
					
/*
 * NO ACTIVITY view contains 
 *		1) all leads created within the given time without activity within the given time (regardless lead state)
 *		2) all OPEN leads without activity within the given time (regardless lead state)
 */ 
IF (@ICL = 'Deals Without Activity')
	INSERT INTO @t
		SELECT 
			l.ItemID, l.FileAs, l.Customer, l.ContactPerson, l.En, 
			l.ItemCreated, l.Price, l.Probability, l.OwnerName, l.link, l.Currency,
			null, null, null, null, null, null, null, null, null, null, dbo.Translate('Task')
		FROM @Leads l
		INNER JOIN vw_Leads VWL ON l.ItemID = VWL.ItemID
		WHERE l.ItemID NOT IN ( SELECT LeadGUID FROM @Activities )-- Without activity (eliminate duplicities)
			 AND	( l.ItemCreated BETWEEN @From and DATEADD(dd, +1, @To) )-- Within given time (case 1)
				
IF (@ICL = 'Deals Without Next Step')
	INSERT INTO @t
		SELECT 
			l.ItemID, l.FileAs, l.Customer, l.ContactPerson, l.En, 
			l.ItemCreated, l.Price, l.Probability, l.OwnerName, l.link, l.Currency,
			a.ItemGUID, a.StartDate, a.EndDate, a.ItemSubject, a.ItemType, 
			a.ItemOwner, a.isInFuture, a.ItemTypeOrder, a.DateToView, a.link, dbo.Translate('Task')
		FROM @Leads l
	     INNER JOIN vw_Leads VWL ON l.ItemID = VWL.ItemID			
	     LEFT JOIN @Activities a ON l.ItemID = A.LeadGUID
		WHERE l.ItemID NOT IN (SELECT leadGUID FROM @Activities WHERE isInFuture = 1)

-- Update some data
UPDATE @t SET dateToView = 
	CASE WHEN startDate is not null and endDate is not null THEN dbo.GetDateInNVarchar(startDate) + ' - ' + dbo.GetDateInNVarchar(endDate) 
		 WHEN startDate is not null and endDate is null THEN dbo.GetDateInNVarchar(startDate)	
		  WHEN startDate is null and endDate is not null THEN dbo.GetDateInNVarchar(endDate)
		  ELSE ''
		  END;

-- Return result table
SELECT * FROM @t</CommandText>
