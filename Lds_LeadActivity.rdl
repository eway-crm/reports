    <DataSet Name="LeadTypes">
      <Query>
        <DataSourceName>LocalDatabase</DataSourceName>
        <CommandText>SELECT ItemID, dbo.TranslateEnum(ItemID) AS en, FileAs AS [Rank]
FROM EnumValues 
WHERE EnumType = (SELECT ItemID FROM EnumTypes WHERE EnumName = N'LeadType')

UNION ALL

SELECT '00000000-0000-0000-0000-000000000000', '('+dbo.Translate('All')+')', NULL

ORDER BY [Rank], ItemID</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
